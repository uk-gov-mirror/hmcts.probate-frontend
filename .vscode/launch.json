    "version": "0.2.0",
    "configurations": [        
        {
            "name": "TEST via yarn",
            "request": "launch",
            "runtimeArgs": [
                "test"
            ],
            "runtimeExecutable": "yarn",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "command": "yarn run test:functional",
            "name": "FUNCTIONAL test via yarn",
            "request": "launch",
            "type": "node-terminal"
        },  
        {
            "command": "yarn run test-e2e",
            "name": "e2e test via yarn",
            "request": "launch",
            "type": "node-terminal"
        },                 
        {
            "args": [
                "--timeout",
                "30000",
                "--colors",
                "${workspaceFolder}/test/accessibility",
                "--reporter",
                "spec",
                "--recursive",
                "--expose-internals",
                "--harmony-generators"
            ],
            "env": {
                "NODE_ENV": "testing",
                "NODE_PATH": ".",
                "LOG_LEVEL": "error",
                "ENABLE_TRACKING": "false"
            },
            "internalConsoleOptions": "openOnSessionStart",
            "name": "Test accessibility",
            "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
